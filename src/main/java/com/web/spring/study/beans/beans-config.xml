<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <bean id="helloBean" class="com.web.spring.study.beans.HelloBean">
        <property name="name">
            <value>peter</value>
        </property>
    </bean>
    
    <bean id="RoundBean" class="com.web.spring.study.beans.RoundBean">
        <property name="radius">
            <value>10</value>
        </property>
    </bean>
    
    <bean id="Husband" class="com.web.spring.study.beans.Husband">
        <property name="name">
            <value>peter</value>
        </property>
        <property name="wife" ref="Wife"/>
    </bean>
    
    <bean id="Wife" class="com.web.spring.study.beans.Wife">
        <property name="name">
            <value>Jane</value>
        </property>
    </bean>
    
    
    
</beans>
